// Ref : http://www.minlplib.org/st_pan1.html
// var x1 >= 0;
// var x2 >= 0;
// var x3 >= 0;
// minimize obj: 1.25*x1 - 2.5*x1^2 - 5*x2^2 + 2.5*x2 - 7.5*x3^2 + 5*x3;

// subject to

// e1:    10*x1 + 0.2*x2 - 0.1*x3 <= 11;

// e2:  - 0.3*x1 + 9*x2 + 0.2*x3 <= 8;

// e3:  - 0.1*x1 + 0.4*x2 + 11*x3 <= 12;

// e4:    6*x1 + 8*x2 + 9*x3 <= 18;

H = [-5  0  0;
      0 -10  0;
      0  0  -15];
f = [1.25;2.5;5];
A = [10    0.2   -0.1;
     -0.3    9    0.2;
     -0.1  0.4     11;
      6      8     9];
b = [11;8;12;18];

[xopt,fopt] = quadprogCLP(H,f,A,b)